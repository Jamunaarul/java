import java.io.*;
import java.util.*;
class factorial{
public static int factorial(int n){
if(n==0){
return 1;
}
return n*factorial(n-1);

}
public static void main(String[]args){
Scanner ob=new Scanner(System.in);
int num=ob.nextInt();
System.out.print("factorial of" +num +"is"+ factorial(num));
}
}
